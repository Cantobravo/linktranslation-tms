      "notation": "standard",
      "onClick": "{{\n(() => {\nconst r = tbl_resources.triggeredRow || tbl_resources.selectedRow;\nif (!r) { showAlert('No row selected.', 'warning'); return; }\nconst to = String(r.resource_email || '').trim();\nif (!to) { showAlert('Row has no email.', 'warning'); return; }\nconst ds = get_email_templates_dd_res.data || [];\nconst val = String(i_email_template_res.selectedOptionValue || '');\nconst hit = ds.find(o => String(o.value) === val) || {};\nconst subject = String(hit.template_email_subject || '');\nconst html = String(hit.template_email_body_html || '');\nconst render = s => String(s).replace(/\\{\\{\\s*(\\w+)\\s*\\}\\}/g, (m,k)=> (k in r ? String(r[k] ?? '') : m));\nconst subjectR = render(subject);\nconst bodyTxt = render(html)\n.replace(/<br\\s*\\/?\\s*>/gi,'\\n')\n.replace(/<\\/p>/gi,'\\n\\n')\n.replace(/<p\\b[^>]*>/gi,'')\n.replace(/<[^>]+>/g,'')\n.replace(/\\n{3,}/g,'\\n\\n')\n.trim() || '\\n';\nconst params = [];\nif (subjectR) params.push('subject=' + encodeURIComponent(subjectR));\nparams.push('body=' + encodeURIComponent(bodyTxt));\nparams.push('x=1');\nconst url = 'mailto:' + encodeURIComponent(to) + '?' + params.join('&');\nnavigateTo(url, 'NEW_WINDOW');\n})()\n}}",
