      "cellBackground": "",
      "columnType": "text",
      "computedValue": "{{(() => { const tableData = tbl_resources.processedTableData || []; return tableData.length > 0 ? tableData.map((currentRow, currentIndex) => (\n  (() => {\n    const raw = String(currentRow.language_pairs || \"\").trim();\n    if (!raw) return \"\";\n\n    // Support both separators depending on your query version\n    const parts = raw.includes(\"; \")\n      ? raw.split(\"; \")\n      : raw.includes(\" | \")\n        ? raw.split(\" | \")\n        : [raw];\n\n    const clean = parts.map(x => x.trim()).filter(Boolean);\n\n    // two pairs per line\n    const lines = [];\n    for (let i = 0; i < clean.length; i += 2) {\n      lines.push([clean[i], clean[i + 1]].filter(Boolean).join(\"; \"));\n    }\n\n    return lines.join(\"\\n\");\n  })()\n)) : \n  (() => {\n    const raw = String(currentRow.language_pairs || \"\").trim();\n    if (!raw) return \"\";\n\n    // Support both separators depending on your query version\n    const parts = raw.includes(\"; \")\n      ? raw.split(\"; \")\n      : raw.includes(\" | \")\n        ? raw.split(\" | \")\n        : [raw];\n\n    const clean = parts.map(x => x.trim()).filter(Boolean);\n\n    // two pairs per line\n    const lines = [];\n    for (let i = 0; i < clean.length; i += 2) {\n      lines.push([clean[i], clean[i + 1]].filter(Boolean).join(\"; \"));\n    }\n\n    return lines.join(\"\\n\");\n  })()\n })()}}",
