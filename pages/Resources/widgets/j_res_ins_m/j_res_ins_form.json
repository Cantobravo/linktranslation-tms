  "onSubmit": "{{\n(async () => {\n  // 1) Insert resource\n  await ins_resource.run();\n\n  // 2) Read inserted ID from ins_resource response\n  const d = ins_resource.data;\n\n  const newId =\n    (Array.isArray(d) && d[0] && (d[0].new_id || d[0].NEW_ID || d[0].insertId || d[0].INSERT_ID))\n    || (d && (d.new_id || d.NEW_ID || d.insertId || d.INSERT_ID))\n    || null;\n\n  const rid = Number(newId);\n\n  if (!rid || Number.isNaN(rid)) {\n    showAlert('Resource inserted but new_id was not returned.', 'warning');\n    return;\n  }\n\n  // 3) Insert pairs (prevent duplicates)\n  const pairsRaw = appsmith.store.ins_pairs || [];\n\n  // de-dup inside the UI list before inserting\n  const uniq = {};\n  for (const p of pairsRaw) {\n    const src = Number(p?.source_language_id);\n    const tgt = Number(p?.target_language_id);\n    if (!src || !tgt || Number.isNaN(src) || Number.isNaN(tgt)) continue;\n    uniq[`${src}-${tgt}`] = { source_language_id: src, target_language_id: tgt };\n  }\n\n  const pairs = Object.values(uniq);\n\n  for (const p of pairs) {\n    await ins_res_pair_if_new.run({\n      resource_id: rid,\n      source_language_id: p.source_language_id,\n      target_language_id: p.target_language_id\n    });\n  }\n\n  // 4) Refresh main table\n  if (toggle_show_inactive_res.isSwitchedOn) {\n    await get_resources_all.run();\n  } else {\n    await get_resources_list.run();\n  }\n\n  // 5) Cleanup + close\n  await storeValue('ins_pairs', []);\n  closeModal('j_res_ins_m');\n  resetWidget('j_res_ins_form', true);\n\n  showAlert('Resource created.', 'success');\n})()\n}}\n",
