  "onSubmit": "{{\n  (async () => {\n    const key = 'pli_save';\n    if (!SubmitShield.lock(key)) return;\n\n    ins_pli.run(\n      async () => {\n        await get_pli_list.run();          // refresh main table\n        SubmitShield.release(key);\n        closeModal('j_pli_ins_m');         // close Insert modal\n        showAlert('Item inserted successfully.', 'success');\n      },\n      (err) => {\n        SubmitShield.release(key);\n        showAlert('Insert failed: ' + (err?.message || JSON.stringify(err)), 'error');\n      }\n    );\n  })()\n}}\n",
