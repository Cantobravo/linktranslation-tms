  "onSubmit": "{{\n  (function () {\n    const key = 'pli_save';\n\n    const src = Number(j_pli_upd_form.formData.source_language_id ?? null);\n    const tgt = Number(j_pli_upd_form.formData.target_language_id ?? null);\n\n    // Guard against violating chk_pli_lang_pair (source must differ from target)\n    if (!src || !tgt) {\n      showAlert('Please select both Source and Target languages.', 'warning');\n      return;\n    }\n    if (src === tgt) {\n      showAlert('Source and Target languages must be different.', 'error');\n      return;\n    }\n\n    if (!SubmitShield.lock(key)) return;\n\n    upd_pli.run(\n      async () => {\n        await get_pli_list.run();       // refresh table\n        SubmitShield.release(key);\n        closeModal('j_pli_upd_m');\n        showAlert('Item updated successfully.', 'success');\n      },\n      (err) => {\n        SubmitShield.release(key);\n        showAlert('Update failed: ' + (err?.message || JSON.stringify(err)), 'error');\n      }\n    );\n  })()\n}}\n",
