      "onClick": "{{\n(async () => {\n  const row = tbl_work_teams.triggeredRow || tbl_work_teams.selectedRow;\n  if (!row) { showAlert('No team row context.', 'warning'); return; }\n\n  await storeValue('wtRow', row);\n\n  // Load current details (IDs for selects)\n  await get_work_team_detail.run();\n\n  // Set customer context for account dropdown\n  await storeValue('_wt_cid_upd', get_work_team_detail.data[0]?.customer_id || null);\n\n  // Prefetch dropdowns\n  await get_customers_dd.run();\n  await get_services_dd.run();\n  await get_units_dd.run();\n  await get_languages_dd.run();\n  await get_currencies_dd.run();\n  await get_cust_acc_dd_upd.run();\n\n  // Reset form and open\n  resetWidget('j_wt_upd_form', true);\n  showModal('j_wt_upd_m');\n})();\n}}\n",
