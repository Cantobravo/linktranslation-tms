      "label": "Manage",
      "labelColor": "#FFFFFF",
      "notation": "standard",
      "onClick": "{{\n(async () => {\n  // 0) Get the team row (prefer the button's row)\n  const team = tbl_work_teams.triggeredRow || tbl_work_teams.selectedRow;\n  if (!team) { showAlert('Select a team row to manage.', 'warning'); return; }\n\n  // 1) Team context for the page\n  await storeValue('wtRow', team);\n\n  // 2) OPTIONAL: reset member filters so PMs see the full team first\n  //    (comment these out if you prefer to keep the last-used filters)\n  resetWidget('member_q', true);\n  resetWidget('member_cert', true);\n  resetWidget('member_avail', true);\n\n  // 3) Load members for this team (uses wtRow inside get_team_members SQL)\n  await get_team_members.run();\n\n  // 4) Choose a default member (first row) so the detail tabs can render immediately\n  const firstMember = (get_team_members.data && get_team_members.data.length) ? get_team_members.data[0] : null;\n  await storeValue('wtmRow', firstMember);\n\n  // 5) Preload detail datasets (queries are resilient if wtmRow is null)\n  await Promise.allSettled([\n    get_wtm_detail.run(),\n    get_wtm_credentials.run(),\n    get_wtm_links.run()\n  ]);\n\n  // 6) Prefetch auxiliary dropdowns used inside the Manage modal\n  //    (resources multi-select; unassigned credential pool)\n  await Promise.allSettled([\n    get_resources_dd?.run?.(),\n    get_uncreds_dd?.run?.()\n  ]);\n\n  // 7) Reset child forms/widgets to avoid stale values\n  resetWidget('j_wtm_cred_ins_form', true);\n  resetWidget('j_wtm_cred_upd_form', true);\n  resetWidget('j_wtm_link_ins_form', true);\n  resetWidget('j_wtm_link_upd_form', true);\n  resetWidget('ms_add_resources', true);\n\n  // 8) Default detail tab\n  await storeValue('u_wtm_tab', 'Basic');\n\n  // 9) Open the Management modal\n  showModal('j_wtm_mgmt_m');\n\n  // 10) Nudge if there are no members yet\n  if (!firstMember) {\n    showAlert('This team has no members yet. Use \"Add Resources\" to add people.', 'info');\n  }\n})();\n}}\n",
