  "onSubmit": "{{\n(async () => {\n  if (appsmith.store.wtm_link_upd_busy) return;\n  await storeValue('wtm_link_upd_busy', true);\n\n  try {\n    // Guard: keep URL non-empty and valid; query will also retain old URL if blank\n    const url = j_wtm_link_upd_form.formData?.link_url;\n    if (!url || String(url).trim() === '') { showAlert('Link URL cannot be empty.', 'warning'); return; }\n    if (!UtilsWT.isUrl(url)) { showAlert('Please enter a valid URL (http/https).', 'warning'); return; }\n\n    await upd_wtm_link.run();\n    await get_wtm_links.run();\n\n    await storeValue('u_wtm_tab','Links');\n\n    const parent = appsmith.store.ctx_parent === 'mgmt' ? 'j_wtm_mgmt_m' : 'j_wtm_upd_m';\n    showModal(parent);\n    closeModal('j_wtm_link_upd_m');\n\n    resetWidget('j_wtm_link_upd_form', true);\n    showAlert('Link updated.', 'success');\n  } catch (e) {\n    showAlert('Update failed: ' + (e?.message || e), 'error');\n  } finally {\n    await storeValue('wtm_link_upd_busy', false);\n  }\n})();\n}}\n",
