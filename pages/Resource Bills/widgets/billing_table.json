      "onClick": "{{\n  (async () => {\n    // 1) Keep the id in store so queries can use it\n    if (!currentRow?.resource_bill_id) { showAlert('Invalid bill row', 'error'); return; }\n    await storeValue('billId', currentRow.resource_bill_id);\n\n    // 2) Ensure dropdown data is ready\n    await get_resources_dd.run();\n\n    // 3) Fetch the authoritative bill row from DB\n    await get_bill_by_id.run();\n    const b = get_bill_by_id.data[0];\n    if (!b) { showAlert('Bill not found', 'error'); return; }\n\n    // 4) Build billRow with email from resources\n    const rRow = get_resources_dd.data.find(x => x.resource_id == b.resource_id);\n    await storeValue('billRow', {\n      resource_bill_id: b.resource_bill_id,\n      resource_id: b.resource_id,\n      resource_bill_date: b.resource_bill_date,\n      resource_bill_due_date: b.resource_bill_due_date,\n      resource_email: rRow ? (rRow.resource_email || '') : ''\n    });\n\n    // 5) Open modal and prefill (resource is read-only)\n    showModal('Update_Modal');\n    upd__resource_id.setSelectedOption({\n      label: rRow ? rRow.resource_full_name : '',\n      value: b.resource_id\n    });\n    upd_resource_email.setValue(rRow ? (rRow.resource_email || '') : '');\n    upd_bill_date.setValue(b.resource_bill_date || '');\n    upd_due_date.setValue(b.resource_bill_due_date || '');\n\n    // 6) Load linked POs for this bill (tbl_upd_pos)\n    await get_bill_purchase_orders.run();\n  })()\n}}\n",
