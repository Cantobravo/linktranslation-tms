  "sourceData": "{{\n  (() => {\n    const opt = (get_uncreds_dd.data || []).find(o => String(o.value) === String(i_uncred_pick.selectedOptionValue));\n    return opt ? {\n      system_code:  opt.system_code,\n      username:     opt.username,\n      alias_email:  opt.alias_email,\n      password_text:opt.password_text,\n      cat_profile:  opt.cat_profile\n    } : { system_code:\"\", username:\"\", alias_email:\"\", password_text:\"\", cat_profile:\"\" };\n  })()\n}}\n\n",
