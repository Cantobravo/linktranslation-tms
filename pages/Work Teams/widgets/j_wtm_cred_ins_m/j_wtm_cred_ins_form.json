  "onSubmit": "{{\n(async () => {\n  if (appsmith.store.wtm_cred_ins_busy) return;\n  await storeValue('wtm_cred_ins_busy', true);\n\n  try {\n    const picked = i_uncred_pick.selectedOptionValue;\n\n    if (picked) {\n      await ins_wtm_credential_from_pool.run();\n      await mark_uncred_assigned.run();\n    } else {\n      await ins_wtm_credential.run();\n    }\n\n    await get_wtm_credentials.run();\n    await get_uncreds_dd.run();\n\n    await storeValue('u_wtm_tab','Credentials');\n\n    const parent = appsmith.store.ctx_parent === 'mgmt' ? 'j_wtm_mgmt_m' : 'j_wtm_upd_m';\n    showModal(parent);\n    closeModal('j_wtm_cred_ins_m');\n\n    resetWidget('i_uncred_pick', true);\n    resetWidget('j_wtm_cred_ins_form', true);\n    showAlert('Credential added.', 'success');\n  } catch (e) {\n    showAlert('Insert failed: ' + (e?.message || e), 'error');\n  } finally {\n    await storeValue('wtm_cred_ins_busy', false);\n  }\n})();\n}}\n",
