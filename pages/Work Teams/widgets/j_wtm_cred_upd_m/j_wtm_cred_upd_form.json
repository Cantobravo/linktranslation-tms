  "onSubmit": "{{\n(async () => {\n  if (appsmith.store.wtm_cred_upd_busy) return;\n  await storeValue('wtm_cred_upd_busy', true);\n\n  try {\n    const picked  = i_uncred_pick_upd.selectedOptionValue;\n    const oldOrig = appsmith.store.wtmCredRow?.origin_uncred_id || tbl_wtm_creds.triggeredRow?.origin_uncred_id;\n\n    if (picked) {\n      if (oldOrig) { await uncred_return_specific.run(); }\n      await upd_wtm_credential_from_pool.run();\n      await mark_uncred_assigned_upd.run();\n    } else {\n      await upd_wtm_credential.run();\n    }\n\n    await get_wtm_credentials.run();\n    await get_uncreds_dd.run();\n\n    await storeValue('u_wtm_tab','Credentials');\n\n    const parent = appsmith.store.ctx_parent === 'mgmt' ? 'j_wtm_mgmt_m' : 'j_wtm_upd_m';\n    showModal(parent);\n    closeModal('j_wtm_cred_upd_m');\n\n    resetWidget('i_uncred_pick_upd', true);\n    resetWidget('j_wtm_cred_upd_form', true);\n    showAlert('Credential updated.', 'success');\n  } catch (e) {\n    showAlert('Update failed: ' + (e?.message || e), 'error');\n  } finally {\n    await storeValue('wtm_cred_upd_busy', false);\n  }\n})();\n}}\n",
