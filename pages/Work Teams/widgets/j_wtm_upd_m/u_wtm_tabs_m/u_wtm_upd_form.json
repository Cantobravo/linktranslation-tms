  "onSubmit": "{{\n(async () => {\n  if (appsmith.store.wtm_upd_busy) return;\n  await storeValue('wtm_upd_busy', true);\n\n  try {\n    // Update the member\n    await upd_wtm_basic.run();\n\n    // Refresh the members table the Management modal uses\n    const editedId = appsmith.store.wtmRow?.work_team_member_id;\n    await get_team_members.run();\n\n    // Keep store.wtmRow synced with the updated record\n    const rows = get_team_members.data || [];\n    const updated = rows.find(r => String(r.work_team_member_id) === String(editedId));\n    if (updated) { await storeValue('wtmRow', updated); }\n\n    showAlert('Member updated.', 'success');\n\n    // Make sure the Management modal is visible, then close only the Edit modal\n    showModal('j_wtm_mgmt_m');                 // ensure we're on the management modal\n    await storeValue('_skip_upd_onclose', true); // tell onClose not to re-open redundantly\n    closeModal('j_wtm_upd_m');                 // return to Management\n\n    // Clean the form for next time\n    resetWidget('u_wtm_upd_form', true);\n  } catch (e) {\n    showAlert('Update failed: ' + (e?.message || e), 'error');\n  } finally {\n    await storeValue('wtm_upd_busy', false);\n  }\n})();\n}}\n",
