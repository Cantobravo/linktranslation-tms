      "onClick": "{{\n(async () => {\n  const row = tbl_work_teams.triggeredRow || tbl_work_teams.selectedRow;\n  if (!row) { showAlert('No team row context.', 'warning'); return; }\n\n  // Keep team context\n  await storeValue('wtRow', row);\n\n  // Load dropdowns first (so Select fields render labels, not raw IDs)\n  const needsDD = !appsmith.store._wt_dd_loaded;\n  if (needsDD) {\n    await Promise.all([\n      get_customers_dd.run(),\n      get_services_dd.run(),\n      get_units_dd.run(),\n      get_languages_dd.run(),\n      get_currencies_dd.run()\n    ]);\n    await storeValue('_wt_dd_loaded', true);\n  }\n\n  // Load detail row (authoritative IDs for the form)\n  await get_work_team_detail.run();\n\n  // Customer accounts depends on customer_id\n  const cid = get_work_team_detail.data?.[0]?.customer_id || null;\n  await storeValue('_wt_cid_upd', cid);\n  await get_cust_acc_dd_upd.run();\n\n  // IMPORTANT:\n  // - Do NOT resetWidget(j_wt_upd_form) here (that reverts your schema/UI to numeric ID fields)\n  // - Do NOT setSourceData(row) (table row has extra columns that can trigger schema regen)\n  j_wt_upd_form.setSourceData(get_work_team_detail.data?.[0] || {});\n  showModal('j_wt_upd_m');\n})()\n}}\n",
