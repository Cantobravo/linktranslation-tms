  "onSubmit": "{{\n  upd_unit.run(\n    async () => {\n      const rows = Number(upd_unit.data?.affectedRows ?? upd_unit.data?.[0]?.affectedRows ?? 0);\n      if (rows === 0) {\n        showAlert('No changes saved (row ID missing or not found).', 'warning');\n      } else {\n        showAlert('Unit updated', 'success');\n      }\n      if (toggle_show_inactive_units?.isSwitchedOn) {\n        await get_units_all.run();\n      } else {\n        await get_units.run();\n      }\n      closeModal('j_unit_update_modal');\n    },\n    (e) => showAlert('Update failed: ' + (e?.message || e), 'error')\n  )\n}}\n",
