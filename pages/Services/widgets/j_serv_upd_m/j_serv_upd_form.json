  "onSubmit": "{{\n  (async () => {\n    const fd = j_serv_upd_form.formData || {};\n    if (!fd.service_id) { showAlert('No service selected', 'warning'); return; }\n    if (!fd.service_name?.trim()) { showAlert('Service name is required', 'warning'); return; }\n\n    await upd_service.run({\n      service_id: fd.service_id,\n      service_name: fd.service_name.trim(),\n      service_status: fd.service_status || 'Active'\n    });\n\n    if (toggle_show_inactive_services?.isSwitchedOn) {\n      await get_services_all.run();\n    } else {\n      await get_services_active.run();\n    }\n\n    closeModal('j_serv_upd_m');\n    showAlert('Service updated successfully', 'success');\n  })()\n}}\n",
