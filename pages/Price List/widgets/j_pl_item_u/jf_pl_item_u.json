  "onSubmit": "{{\n(async () => {\n  const row = appsmith.store.plItemRow;\n  if (!row?.price_list_item_id) { showAlert(\"No row selected.\", \"warning\"); return; }\n\n  const fd = jf_pl_item_u.formData || {};\n\n  if (!fd.service_id || !fd.unit_id || !fd.source_language_id || !fd.target_language_id) {\n    showAlert(\"Service, Unit, Source Language and Target Language are required.\", \"warning\");\n    return;\n  }\n\n  try {\n    await upd_pl_item.run({\n      price_list_item_id: row.price_list_item_id,\n      service_id: fd.service_id,\n      unit_id: fd.unit_id,\n      source_language_id: fd.source_language_id,\n      target_language_id: fd.target_language_id,\n      sell_rate: (fd.sell_rate ?? row.sell_rate ?? 0),\n      cost_rate: (fd.cost_rate ?? row.cost_rate ?? 0),\n      is_active: (fd.is_active ?? row.is_active ?? 1),\n      notes: fd.notes\n    });\n\n    closeModal(\"j_pl_item_u\");\n    resetWidget(\"jf_pl_item_u\");\n\n    if (appsmith.store.plCtx?.scope === \"BENCHMARK\") {\n      await get_pl_items_bm.run();\n    } else {\n      await get_pl_items_acc.run();\n    }\n\n    showAlert(\"Updated.\", \"success\");\n  } catch (e) {\n    showAlert(\"Update failed.\", \"error\");\n  }\n})()\n}}\n",
