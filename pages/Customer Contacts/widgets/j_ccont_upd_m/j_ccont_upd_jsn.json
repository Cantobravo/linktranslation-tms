  "onSubmit": "{{\n(async () => {\n  try {\n    const id = Number(j_ccont_upd_jsn.formData?.customer_contact_id || 0);\n    if (!id) {\n      showAlert('Missing customer_contact_id. Please reopen the edit modal from the row.', 'warning');\n      return;\n    }\n\n    const emailNewRaw = (j_ccont_upd_jsn.formData?.customer_contact_email || '').trim();\n    const emailNew = emailNewRaw.toLowerCase();\n\n    const emailOldRaw =\n      (appsmith.store.ccontRow?.customer_contact_email || '').trim();\n    const emailOld = emailOldRaw.toLowerCase();\n\n    // Only check duplicates if email actually changed\n    if (emailNew && emailNew !== emailOld) {\n      const r = await chk_ccont_email_dupe.run({ email: emailNewRaw, exclude_id: id });\n      const cnt = Number(r?.[0]?.cnt || 0);\n\n      if (cnt > 0) {\n        showAlert('This email is already used by another contact. Please use a different email.', 'warning');\n        return;\n      }\n    }\n\n    await upd_ccont.run();\n\n    // âœ… Refresh the table source actually used by the page\n    await get_ccont_list.run();\n\n    showAlert('Contact updated successfully.', 'success');\n    closeModal('j_ccont_upd_m');\n\n  } catch (e) {\n    showAlert(\n      `Update failed: ${e?.message || e?.data?.message || 'Unknown error'}`,\n      'error'\n    );\n  }\n})()\n}}\n",
