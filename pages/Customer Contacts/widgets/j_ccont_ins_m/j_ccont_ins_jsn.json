  "onSubmit": "{{\n(async () => {\n  try {\n    const emailRaw = (j_ccont_ins_jsn.formData?.customer_contact_email || '').trim();\n\n    // Duplicate email check (only if email provided)\n    if (emailRaw) {\n      const r = await chk_ccont_email_dupe.run({ email: emailRaw, exclude_id: -1 });\n      const cnt = Number(r?.[0]?.cnt || 0);\n\n      if (cnt > 0) {\n        showAlert('This email is already used by another contact. Please use a different email.', 'warning');\n        return;\n      }\n    }\n\n    await ins_ccont.run();\n\n    // âœ… Refresh the table source actually used by the page\n    await get_ccont_list.run();\n\n    showAlert('Contact added successfully.', 'success');\n    closeModal('j_ccont_ins_m');\n\n  } catch (e) {\n    showAlert(\n      `Insert failed: ${e?.message || e?.data?.message || 'Unknown error'}`,\n      'error'\n    );\n  }\n})()\n}}\n",
