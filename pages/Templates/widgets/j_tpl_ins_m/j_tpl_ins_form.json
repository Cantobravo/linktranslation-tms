  "onSubmit": "{{\n  (async () => {\n    // simple submit guard\n    if (appsmith.store.tpl_ins_busy) return;\n    await storeValue('tpl_ins_busy', true);\n\n    try {\n      const fd = j_tpl_ins_form.formData || {};\n      if (!fd.template_name || !fd.template_type) {\n        showAlert(\"Please fill Template Name and Type.\", \"warning\");\n        return;\n      }\n\n      // run insert\n      await ins_template.run();\n\n      // success UX\n      showAlert(\"Template created successfully.\", \"success\");\n\n      // refresh main list\n      await get_templates_list.run();\n\n      // close modal, clean form state\n      closeModal('j_tpl_ins_m');\n      resetWidget('j_tpl_ins_form', true);\n      await storeValue('tpl_ins_data', {\n        template_name: \"\",\n        template_type: \"\",\n        template_description: \"\",\n        template_is_active: true,\n        template_version: 1,\n        template_from_field: \"\",\n        template_email_to_field: \"\",\n        template_email_bcc_field: \"\",\n        template_email_subject: \"\",\n        template_email_body_html: \"\",\n        template_signature_field: \"\",\n        template_header_html: \"\",\n        template_footer_html: \"\",\n        template_pdf_body: \"\",\n        template_vars: []\n      });\n    } catch (e) {\n      showAlert(\"Insert failed: \" + (e?.message || e), \"error\");\n    } finally {\n      await storeValue('tpl_ins_busy', false);\n    }\n  })()\n}}\n",
