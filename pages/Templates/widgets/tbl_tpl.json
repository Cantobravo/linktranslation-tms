      "onClick": "{{\n(async () => {\n  const row = tbl_tpl.triggeredRow || tbl_tpl.selectedRow;\n  if (!row?.template_id) { showAlert(\"No template selected.\", \"warning\"); return; }\n\n  // Store-First: always set both\n  await storeValue(\"tplRow\", row);\n  await storeValue(\"tplUpdObj\", { ...row });\n\n  // Reset focus key for placeholder insertion\n  await storeValue(\"tplFocusKey\", null);\n\n  showModal(\"m_tpl_upd\");\n\n  // Reset AFTER modal open so JSON form re-renders from sourceData = tplUpdObj\n  resetWidget(\"jf_tpl_upd\", true);\n})()\n}}\n",
