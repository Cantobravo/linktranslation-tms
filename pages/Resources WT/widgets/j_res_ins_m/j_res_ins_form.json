  "onSubmit": "{{\n(async () => {\n  const email = (j_res_ins_form.formData?.resource_email || '').trim();\n  if (!email) { showAlert('Email is required.', 'warning'); return; }\n\n  // Normalize Dropbox URL\n  const rawDoc  = j_res_ins_form.formData?.resource_documentation || '';\n  const normDoc = rawDoc ? UtilsRes.normalizeDropboxUrl(rawDoc) : '';\n  if (rawDoc && !UtilsRes.isUrl(normDoc)) {\n    showAlert('Please enter a valid Dropbox URL (http/https).', 'warning'); return;\n  }\n  j_res_ins_form.formData.resource_documentation = normDoc;\n\n  if (appsmith.store._res_ins_busy) return;\n  await storeValue('_res_ins_busy', true);\n\n  try {\n    // 1) Pre-check duplicate by email\n    const dupe = await get_res_id_by_email.run();\n    const dupeId =\n      Number(dupe?.[0]?.resource_id || dupe?.data?.[0]?.resource_id || 0);\n    if (dupeId) {\n      showAlert(`Email already exists (resource #${dupeId}).`, 'warning'); return;\n    }\n\n    // 2) Insert resource\n    const res = await ins_resource.run();\n\n    // 3) Resolve new ID\n    let newId =\n      Number(res?.insertId) ||\n      Number(res?.data?.insertId) ||\n      Number(res?.[0]?.insertId) || 0;\n\n    if (!newId) {\n      const r2 = await get_res_id_by_email.run();\n      newId = Number(r2?.[0]?.resource_id || r2?.data?.[0]?.resource_id || 0);\n    }\n    if (!newId) { showAlert('Could not resolve new resource ID.', 'error'); return; }\n\n    // 4) Insert staged language pairs\n    const pairs = appsmith.store.ins_pairs || [];\n    for (const p of pairs) {\n      await ins_res_pair_if_new.run({\n        resource_id: newId,\n        source_language_id: p.source_language_id,\n        target_language_id: p.target_language_id\n      });\n    }\n\n    // 5) Refresh + close\n    await storeValue('ins_pairs', []);\n    await get_resources_list.run();\n    showAlert('Resource created.', 'success');\n    closeModal('j_res_ins_m');\n    resetWidget('j_res_ins_form', true);\n  } catch (e) {\n    showAlert('Insert failed: ' + (e?.message || e), 'error');\n  } finally {\n    await storeValue('_res_ins_busy', false);\n  }\n})()\n}}\n",
