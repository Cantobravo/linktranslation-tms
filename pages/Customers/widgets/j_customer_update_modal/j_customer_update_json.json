  "onSubmit": "{{\n  SubmitShield.runOnce(async () => {\n    await chk_cust_upd.run();\n    const dup = Number((chk_cust_upd.data?.[0]?.cnt) || 0);\n    if (dup > 0) {\n      showAlert('Customer name already exists', 'error');\n      return;\n    }\n\n    await upd_customer.run();\n\nawait get_customers_list.run();\n\n\n    // If user changed to \"no\", switch view so they can see it immediately\n    const v = (j_customer_update_json.formData?.is_customer || '').toLowerCase();\n    if (v === 'no' && toggle_show_inactive_customers?.setValue) {\n      toggle_show_inactive_customers.setValue(true);\n    }\n    if (v === 'yes' && toggle_show_inactive_customers?.setValue) {\n      toggle_show_inactive_customers.setValue(false);\n    }\n\n    closeModal('j_customer_update_modal');\n    resetWidget('j_customer_update_json', true);\n\n    showAlert('Customer updated', 'success');\n  })\n}}\n",
