      "label": "Edit",
      "labelColor": "#FFFFFF",
      "notation": "standard",
      "onClick": "{{\n  (async () => {\n    const row = tbl_po.triggeredRow;\n    if (!row?.purchase_order_id) {\n      showAlert('No row context.', 'warning');\n      return;\n    }\n\n    // Store-first (stable row context)\n    await storeValue('poId', row.purchase_order_id);\n\n    // Load dropdown options ONLY once per session (unless you manually clear store)\n    const need = (k) => !(Array.isArray(appsmith.store[k]) && appsmith.store[k].length > 0);\n\n    const tasks = [];\n    if (need('opt_resources')) tasks.push(get_resources_dd.run().then(() => storeValue('opt_resources', get_resources_dd.data || [])));\n    if (need('opt_services'))  tasks.push(get_services_dd.run().then(() => storeValue('opt_services',  get_services_dd.data  || [])));\n    if (need('opt_units'))     tasks.push(get_units_dd.run().then(() => storeValue('opt_units',     get_units_dd.data     || [])));\n    if (need('opt_langs'))     tasks.push(get_languages_dd.run().then(() => storeValue('opt_langs',     get_languages_dd.data || [])));\n\n    if (tasks.length) {\n      await Promise.allSettled(tasks);\n    }\n\n    // Load PO row (MANUAL query)\n    const ok = await get_po_by_id.run();\n    if (!ok || !get_po_by_id.data?.length) {\n      showAlert('Could not load purchase order details', 'error');\n      return;\n    }\n\n    // Open modal\n    showModal('j_po_upd_m');\n  })()\n}}\n",
      "originalId": "customColumn1",
