  "onSubmit": "{{\n  (() => {\n    const form = (typeof JSON_Role_Insert !== 'undefined') ? JSON_Role_Insert : j_role;\n    const name = (form.formData?.role_name ?? '').toString().trim();\n    if (!name) { showAlert('Role name is required.', 'warning'); return; }\n\n    add_role.run(\n      () => {\n        get_roles.run(\n          () => get_roles_perms_summary.run(\n            () => get_roles_perms_flat.run(\n              () => { \n                showAlert('Role created', 'success');\n                resetWidget(form.widgetName || 'JSON_Role_Insert', true);\n                closeModal('j_role_insert_modal');\n              },\n              (e)=>showAlert('roles-flat refresh failed: '+(e?.message||e), 'error')\n            ),\n            (e)=>showAlert('roles-summary refresh failed: '+(e?.message||e), 'error')\n          ),\n          (e)=>showAlert('roles refresh failed: '+(e?.message||e), 'error')\n        );\n      },\n      (e)=>showAlert('Create failed: '+(e?.message||e), 'error')\n    );\n  })()\n}}\n",
